{
   "name": "autentication",
   "version": "1.0.0",
   "main": "index.js",
   "scripts": {
      "dev": "cross-env NODE_ENV=development nodemon --exec tsx -r tsconfig-paths/register src/index.ts",
      "start": "cross-env NODE_ENV=production node ./src/index.js",
      "preview": "cross-env NODE_ENV=production node ./dist/src/index.js",
      "build": "rimraf dist && swc src --out-dir dist --config-file ./.swcrc --ignore **/*.test.ts",
      "lint": "npx eslint ./src",
      "format": "npx prettier ./src --write",
      "prepare": "husky",
      "test": "jest -experimental-vm-modules",
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:generate": "cross-env NODE_ENV=development typeorm-ts-node-commonjs migration:generate",
      "migration:dev": "cross-env NODE_ENV=development typeorm-ts-node-commonjs migration:run -d dist/src/data-source.js",
      "migration:run": "cross-env NODE_ENV=development typeorm-ts-node-commonjs migration:run -d src/data-source.js"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "description": "",
   "devDependencies": {
      "@commitlint/cli": "^20.1.0",
      "@commitlint/config-conventional": "^20.0.0",
      "@eslint/js": "^9.38.0",
      "@jest/globals": "^30.2.0",
      "@swc/cli": "^0.7.8",
      "@swc/core": "^1.13.5",
      "@swc/jest": "^0.2.39",
      "@types/cookie-parser": "^1.4.9",
      "@types/cors": "^2.8.19",
      "@types/express": "^5.0.3",
      "@types/http-errors": "^2.0.5",
      "@types/jest": "^30.0.0",
      "@types/jsonwebtoken": "^9.0.10",
      "@types/morgan": "^1.9.10",
      "@types/node": "^24.8.1",
      "@types/supertest": "^6.0.3",
      "eslint": "^9.38.0",
      "husky": "^9.1.7",
      "jest": "^30.2.0",
      "jest-mock-extended": "^4.0.0",
      "lint-staged": "^16.2.4",
      "nodemon": "^3.1.10",
      "prettier": "3.6.2",
      "supertest": "^7.1.4",
      "ts-jest": "^29.4.5",
      "ts-node": "10.9.2",
      "tsconfig-paths": "^4.2.0",
      "tsx": "^4.20.6",
      "typescript": "5.9.3",
      "typescript-eslint": "^8.46.1"
   },
   "dependencies": {
      "@types/bcrypt": "^6.0.0",
      "bcrypt": "^6.0.0",
      "cookie-parser": "^1.4.7",
      "cors": "^2.8.5",
      "cross-env": "^10.1.0",
      "dotenv": "^17.2.3",
      "express": "^5.1.0",
      "express-jwt": "^8.5.1",
      "jsonwebtoken": "^9.0.2",
      "jwks-rsa": "^3.2.0",
      "morgan": "^1.10.1",
      "pg": "^8.16.3",
      "reflect-metadata": "^0.2.2",
      "rimraf": "^6.0.1",
      "rsa-pem-to-jwk": "^1.1.3",
      "typeorm": "0.3.27",
      "winston": "^3.18.3",
      "zod": "^4.1.12"
   }
}